{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "getdishes": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/dishes"
      }
    },
    "AddDishes": {
      "matchCondition": {
        "methods": [ "POST" ],
        "route": "/api/dishes"
      }
    },
    "UpdateDishes": {
      "matchCondition": {
        "methods": [ "PUT" ],
        "route": "/api/dishes"
      }
    },
    "DeleteDish": {
      "matchCondition": {
        "methods": [ "DELETE" ],
        "route": "/api/dishes"
      }
    }
  }
}